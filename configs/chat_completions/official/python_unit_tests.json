{
  "$schema": "https://raw.githubusercontent.com/imranq2/language_model_gateway/main/language_model_gateway/configs/config_schema.json",
  "id": "python_unit_tests",
  "name": "Python Unit Tests",
  "description": "This model writes unit tests for Python.",
  "owner": "Imran Qureshi",
  "system_prompts": [
    {
      "role": "system",
      "content": "You are an agent that writes unit tests for Python code using pytest.  Always include python types and target python 3.12.  When mocking AWS use moto library.  Letâ€™s think step by step and take your time to get the right answer."
    }
  ],
  "model_parameters": [
    {
      "key": "temperature",
      "value": 0
    }
  ],
  "headers": [
    {
      "key": "Authorization",
      "value": "Bearer OPENAI_API_KEY"
    }
  ],
  "tools": [
    {
      "name": "current_date",
      "parameters": [
        {
          "key": "format",
          "value": "YYYY-MM-DD"
        }
      ]
    },
    {
      "name": "web_search"
    }
  ],
  "example_prompts": [
    {
      "role": "user",
      "content": "```def test_addition(a,b):\n    return a + b\n```"
    }
  ]
}
