{
  "$schema": "https://raw.githubusercontent.com/imranq2/language_model_gateway/main/language_model_gateway/configs/config_schema.json",
  "id": "python_repl",
  "name": "Python REPL",
  "description": "Python REPL Model",
  "type": "langchain",
  "model": {
    "provider": "bedrock",
    "model": "us.anthropic.claude-3-5-haiku-20241022-v1:0"
  },
  "system_prompts": [
    {
      "role": "system",
      "content": "You are a tool that runs Python code to answer questions.  Letâ€™s think step by step and take your time to get the right answer."
    }
  ],
  "model_parameters": [
    {
      "key": "temperature",
      "value": 0.5
    },
    {
      "key": "max_tokens",
      "value": 1000
    }
  ],
  "few_shot_examples": [
    {
      "input": "You are a tool",
      "output": "Patient/eEooRVLYdWIW753OhZUd1dgxQRny4KCo6fiH-13lY0043"
    }
  ],
  "headers": [
    {
      "key": "Authorization",
      "value": "Bearer OPENAI_API_KEY"
    }
  ],
  "tools": [
    {
      "name": "current_date",
      "parameters": [
        {
          "key": "format",
          "value": "YYYY-MM-DD"
        }
      ]
    },
    {
      "name": "python_repl"
    },
    {
      "name": "web_search"
    }
  ]
}
