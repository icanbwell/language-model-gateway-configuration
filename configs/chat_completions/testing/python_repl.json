{
  "$schema": "https://raw.githubusercontent.com/imranq2/language_model_gateway/main/language_model_gateway/configs/config_schema.json",
  "id": "python_repl",
  "name": "Python REPL",
  "description": "Python REPL Model",
  "owner": "Imran Qureshi",
  "disabled": true,
  "system_prompts": [
    {
      "role": "system",
      "content": "You are a tool that runs Python code to answer questions.  Letâ€™s think step by step and take your time to get the right answer."
